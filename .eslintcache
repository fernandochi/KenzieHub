/users/:perPage/:page\"\n        render={(props) =>\n          isAuthorized ? (\n            <Users {...props} />\n          ) : (\n            history.push(\"/user-registration\")\n          )\n        }\n      />\n      <Route\n        path=\"/profile\"\n        render={(props) =>\n          isAuthorized ? (\n            <Profile {...props} />\n          ) : (\n            history.push(\"/user-registration\")\n          )\n        }\n      />\n      <Route\n        path=\"/portfolio\"\n        render={(props) =>\n          isAuthorized ? (\n            <Portfolio {...props} />\n          ) : (\n            history.push(\"user-registration\")\n          )\n        }\n      />\n      <Route\n        path=\"/technologies\"\n        render={(props) =>\n          isAuthorized ? (\n            <Technologies {...props} />\n          ) : (\n            history.push(\"user-registrarion\")\n          )\n        }\n      />\n    </Switch>\n  );\n};\n\nexport default MainRoutes;\n",["95","96"],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/store/modules/token/actions.js",[],["97","98"],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/pages/userRegistration/index.jsx",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/pages/portfolio/index.jsx",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/pages/homePage/index.jsx",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/pages/homePage/styles.js",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/components/aboutUs/index.jsx",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/components/devCard/index.jsx",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/pages/users/index.jsx",["99"],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/components/menu/index.jsx",["100","101","102","103"],"import { useHistory, useLocation } from \"react-router-dom\";\nimport { useState, useEffect } from \"react\";\nimport { MenuUl, MenuLi } from \"./style\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nconst Menu = () => {\n  const token = useSelector((state) => state.booleanToken);\n  const history = useHistory();\n\n  const handleLogin = () => {\n    if (token) {\n      localStorage.clear();\n      history.push(\"/login\");\n    }\n    history.push(\"/login\");\n  };\n\n  const handlUser = () => {\n    if (token) {\n      return history.push(\"/users/10/1\");\n    }\n    history.push(\"/users-list\");\n  };\n\n  const handlePath = (event, path) => {\n    history.push(`/${path}`);\n  };\n\n  return (\n    <div>\n      <MenuUl>\n        <MenuLi onClick={handleLogin}>{token ? \"Logout\" : \"Login\"}</MenuLi>\n        <MenuLi onClick={(event) => handlePath(event, \"user-registration\")}>\n          Cadastro\n        </MenuLi>\n        <MenuLi onClick={handlUser}>Usuários</MenuLi>\n        {token && (\n          <>\n            <MenuLi onClick={(event) => handlePath(event, \"profile\")}>\n              Edição de Perfil\n            </MenuLi>\n            <MenuLi onClick={(event) => handlePath(event, \"portfolio\")}>\n              Portfólio\n            </MenuLi>\n            <MenuLi onClick={(event) => handlePath(event, \"technologies\")}>\n              Tecnologias\n            </MenuLi>\n          </>\n        )}\n      </MenuUl>\n    </div>\n  );\n};\n\nexport default Menu;\n","/home/fernando/atividade/Q2/kenziehub/kenziehub/src/store/index.js",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/store/modules/users/thunks.js",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/store/modules/users/reducer.js",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/App.js",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/pages/technologies/index.jsx",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/store/modules/login/actions.js",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/store/modules/users/actions.js",[],"/home/fernando/atividade/Q2/kenziehub/kenziehub/src/components/cardUsers/index.jsx",[],"/home/fernando/ativida